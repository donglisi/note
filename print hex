#include <stdio.h>
#include <stdint.h>

void hex4(unsigned char c)
{
	if (c < 10)
		putchar(c + 48);
	else 
		putchar(c + 55);
}

void hex8(unsigned char c)
{
	hex4(c >> 4);
	hex4(c & 0xf);
}

void hex64(void *x)
{
	unsigned char *c = x;
	for (int i = 7; i >= 0; i--) {
		hex8(c[i]);
	}
	putchar('\n');
}

uint64_t x = 0x41424344F5;
int main(int argc, char *argv[])
{

	hex64(&x);
	return 0;
}
